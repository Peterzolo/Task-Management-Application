  "description": "Technical Assessment for backend developer",
  "scripts": {
    "prestart": "npm run build",
    "start": "node build/bin/www/server.js",
    "prewatch": "npm run build",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run nodemon-watch\"",
    "nodemon-watch": "nodemon src/bin/www/server.ts",
    "clean": "",
    "build": "npm run clean && npm run build-ts && npm run copy-assets && npm run seed",
    "watch-ts": "tsc -w",
    "lint": "npx eslint --fix src/",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "prettify": "prettier --write src/**/*.ts",
    "build-ts": "tsc --project .",
    "copy-assets": "node scripts/copy_assets.js",
    "seed": "node build/databases/seed/index.js"
  },
